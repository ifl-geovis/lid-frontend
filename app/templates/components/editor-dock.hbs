{{!-- Editor dock --}}
<div class="flex-20">
  <div class="editor-dock">

    {{#paper-button primary=true fab=true onClick=(action "publishSite")}}
      {{paper-icon "file upload"}}
    {{/paper-button}} 
    <br>
    
    {{#paper-button fab=true onClick=(action "previewSite")}}
      {{paper-icon "pageview"}}      
    {{/paper-button}}
    <br>

    {{#paper-button fab=true raised=true onClick=(action "openHelper")}}
      {{paper-icon "help outline"}}
    {{/paper-button}}    
    <br>
    
    {{#if updateRegion}}
      {{#paper-button fab=true onClick=(action "updateRegion" model)}}
        {{paper-icon "checkcircle"}}
      {{/paper-button}}    
      <br>
    {{/if}}

    {{#if updateTopic}}
      {{#paper-button fab=true onClick=(action "updateTopic" model)}}
        {{paper-icon "checkcircle"}}
      {{/paper-button}}    
      <br>
    {{/if}}

    {{#if updateExcursion}}
      {{#paper-button fab=true onClick=(action "updateExcursion" model)}}
        {{paper-icon "checkcircle"}}
      {{/paper-button}}    
      <br>
    {{/if}}    
  
    {{#if deleteRegion}}
      {{#paper-button warn=true fab=true onClick=(action "deleteRegion" model)}}
        {{paper-icon "delete forever"}}
      {{/paper-button}}
      <br>
    {{/if}}

    {{#if deleteTopic}}
      {{#paper-button warn=true fab=true onClick=(action "deleteTopic" model)}}
        {{paper-icon "delete forever"}}
      {{/paper-button}}
      <br>
    {{/if}}

    {{#if deleteExcursion}}
      {{#paper-button warn=true fab=true onClick=(action "deleteExcursion" model)}}
        {{paper-icon "delete forever"}}
      {{/paper-button}}
      <br>
    {{/if}}
    
  </div>    
</div>