
  {{#paper-menu as |menu|}}
    {{#menu.trigger}}
       <h4 class="meta-editor-button">alle anzeigen</h4>
    {{/menu.trigger}}

    {{#menu.content width=5 as |content|}}
      <h4 class="centered">Quellenverzeichnis {{model.id}}</h4>

      {{#paper-list}}
        {{#each references as |reference|}}
        {{#paper-item class="md-1-line"}}
            <div class="md-list-item-text">
              <p>{{reference}}</p>
            </div>
          {{/paper-item}}
        {{/each}}
      {{/paper-list}}

    {{/menu.content}}
  {{/paper-menu}}

